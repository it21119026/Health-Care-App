<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#120639"
    tools:context=".InsuaranceMainFunc">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insuarance"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <TextView
        android:id="@+id/enrolment"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/round_textview_corners"
        android:src="@drawable/round_textview_corners"
        android:text="Enrolment"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.28" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/enrol"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:src="@drawable/enrolment"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/enrolment"
        app:layout_constraintEnd_toEndOf="@+id/enrolment"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="@+id/enrolment"
        app:layout_constraintTop_toTopOf="@+id/enrolment"
        app:layout_constraintVertical_bias="0.83" />

    <TextView
        android:id="@+id/aboutus"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/round_textview_corners"
        android:text="About Us"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/enrolment"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.28" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/aboutu"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:src="@drawable/aboutus"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/aboutus"
        app:layout_constraintEnd_toEndOf="@+id/aboutus"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="@+id/aboutus"
        app:layout_constraintTop_toTopOf="@+id/aboutus"
        app:layout_constraintVertical_bias="0.833" />


    <TextView
        android:id="@+id/privacy"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="156dp"
        android:background="@drawable/round_textview_corners"
        android:text="Privacy Policy"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enrolment"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/sup"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginBottom="156dp"
        android:background="@drawable/round_textview_corners"
        android:text="Support"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/privacy"
        app:layout_constraintTop_toBottomOf="@+id/aboutus"
        app:layout_constraintVertical_bias="1.0" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/privaa"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:src="@drawable/privacy"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/privacy"
        app:layout_constraintEnd_toEndOf="@+id/privacy"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="@+id/privacy"
        app:layout_constraintTop_toTopOf="@+id/privacy"
        app:layout_constraintVertical_bias="0.814" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/suppp"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:src="@drawable/support"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/sup"
        app:layout_constraintEnd_toEndOf="@+id/sup"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/sup"
        app:layout_constraintTop_toTopOf="@+id/sup"
        app:layout_constraintVertical_bias="0.814" />

</androidx.constraintlayout.widget.ConstraintLayout>